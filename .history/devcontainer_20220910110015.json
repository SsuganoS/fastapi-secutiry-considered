{
	"name": "Existing Docker Compose (Extend)",
	"dockerComposeFile": [
        "./docker-compose.yml"
	],
    "service": "fastapi",
    "workspaceFolder": "/app/fastapi",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/sh",
        "go.gopath": "/go",
        "go.inferGopath": true,
        "go.useLanguageServer": true
    },
	"extensions": [
	    "golang.go",
	    "wholroyd.jinja"
	]
}